{
  "name": "@r6ru/r6ru",
  "private": true,
  "main": "bootstrap.js",
  "scripts": {
    "bootstrap": "lerna bootstrap --scope=@r6ru/$PACKAGE_NAME --include-filtered-dependencies",
    "build": "lerna run build --scope=@r6ru/$PACKAGE_NAME --include-filtered-dependencies",
    "postinstall": "yarn bootstrap && yarn build",
    "start": "node ./packages/$PACKAGE_NAME/build/index.js",
    "front-publish": "ln ./packages/$PACKAGE_NAME/build ./front-publish"
  },
  "engines": {
    "node": ">=10.16.x",
    "yarn": ">=1.17.x"
  },
  "dependencies": {
    "lerna": "^3.13.1",
    "typescript": "^3.3.3333"
  },
  "devDependencies": {
    "@types/node": "^12.0.2",
    "tslint": "^5.13.1"
  }
}
